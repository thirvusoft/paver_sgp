 "html": "<div>\n{% if letter_head and not no_letterhead -%}\n{% if print_settings.repeat_header_footer %}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{%- endif %}\n{%- endif %}\n<hr style=\"width;100%; border-bottom: 1px solid grey;\">\n<div class=\"master\" style=\"border: 1px solid grey;\">\n    <div style=\"width;100%; border-bottom: 1px solid grey; background-color: orange; font-size:20px; color:black; height:40px; border-top:5px solid orange\" align=\"center\"><b>QUOTATION</b></div>\n    <div>\n        <table  width=\"100%\" style=\"color:black; font-size:15px;\">\n            <tr>\n                <td width=\"13%\"><b>To</b></td>\n                <td>:</td>\n                <td><b>{{ doc.party_name }}</b><br><br>\n                    {{ doc.address_display }}</td>\n                    <td width=\"5%\"></td>\n                    <td width=\"5%\"></td>\n                <td width=\"20%\" ><b>Quotation :<br>No.</b><br><br>\n                <b>Date&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;:</b>\n                </td>\n                <td width=\"20%\">{{ doc.name }}<br><br>\n                    {{  doc.get_formatted(\"transaction_date\") }}\n                </td>\n            </tr>\n        </table>\n        <div style=\"font-size:15px; color:black; padding:5px\">\n            <table style=\"color:black\">\n            <tr>\n                <td align=\"left\" width=\"20%\"><b>Kind Attn :</b><br></td>\n                <td align=\"left\"></td>\n            </tr>\n            <tr>\n                <td align=\"left\" width=\"20%\"><b>Sub :</b></td>\n                <td align=\"left\">Rate Quotation For Paver Blocks.</td>\n            </tr>\n            </table>\n            <table style=\"color:black\">\n            <tr>\n                <td><b>Sir / Madam,</b><br></td>\n                <td></td>\n            </tr>\n            <tr>\n                <td>&nbsp; Thank you very much for your enquiry, We have great pleasure to submitting for our least quotations.</td>\n                <td></td>\n            </tr>\n            </table>\n        </div>\n    </div>\n    <div style=\"padding:5px;\">\n        {% set serial_no=[] %}\n        <table style=\"border:1px solid grey; color:black;\">\n            {% set tablelength=10 %}\n            <tr style=\"border-bottom:1px solid grey\">\n                <td style=\"background-color:orange !important;\"><b>S.No.</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange !important;\" align=\"center\"><b>Particulars</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange !important;\" align=\"center\"><b>Qty</b></td>\n               <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Uom</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Rate</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Amount</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>GST%</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>GST Amount</b></td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\"><b>Total Amount</b></td>\n            </tr>\n            <!--for loop for items-->\n            \n            {% for item in doc.items %}\n                {% set _=serial_no.append(1) %}\n            <tr>\n                <td align=\"left\" width=\"6%\">{{ serial_no|length }}</td>\n                <td align=\"left\" style=\"border-left:1px solid grey;\" width=\"36%\">{{ item.item_name }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"10%\">{{ item.qty}}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"6%\">{{ item.uom }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\">{{ \"%.2f\"|format(item.rate) }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\">{{ \"%.2f\"|format(item.amount) }}</td>\n                 <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\"></td>\n                  <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\"></td>\n                   <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\"></td>\n            </tr>\n            {% endfor %}\n            \n             </table>\n            </div>\n            <div class=\"secondhalf\" width=\"100%\">\n            <table align=\"right\" style=\"color:black\" width=\"100%\">\n            <tr align=\"right\">\n                <td width=\"20%\"></td>\n                <td width=\"20%\"></td>\n                <td width=\"20%\"></td>\n                <td width=\"20%\"><b>GST :</b></td>\n                <td width=\"20%\" align=\"right\">{{ \"%.2f\"|format(doc.total_taxes_and_charges) }}</td>\n            </tr>\n            <tr align=\"right\" >\n                <td width=\"20%\"></td>\n                <td width=\"20%\"></td>\n                <td width=\"20%\"></td>\n                <td width=\"20%\"><b>Grand Total :</b></td>\n                <td width=\"20%\" align=\"right\">{{ \"%.2f\"|format(doc.grand_total) }}</td>\n            </tr>\n        </table>\n    </div>\n    <div width=\"100%\" style=\"color:black; font-size:15px;padding:5px;\">\n        <p width=\"100%\"><b>Terms & Conditions</b><br>\n        <!--1.Provision of water, electricity and staying room for labours should be provided by the Site Owner.<br>-->\n        <!--2.Site cleaning and leveling site owner scope.<br>-->\n        <!--3.Bedding 6mm chips and joint filling p sand site owner scope..<br>-->\n        <!--4.25% Payment in advance.<br>-->\n        <!--5.Delivery 2 weeks from date of PO.<br>-->\n        {{doc.terms or \" \"}}<br>\n        </p>\n    </div>\n    <div style=\"padding:5px;\">\n        <table width=100% style=\"color:black; border:1px solid grey; border-top:0px;\">\n            {% set bank = frappe.get_doc(\"Bank Account\", \"Sri Ganapathy Fly Ash Bricks - Bank of India\")%}\n            <tr>\n                <td align=\"right\" style=\"width:50%; color:black; background-color:orange; font-size:15px; border: 1px solid grey; border-right: 0px;\">Bank</td>\n                <td align=\"left\" style=\"width:50%; color:black; background-color:orange; font-size:15px; border: 1px solid grey; border-left: 0px;\">Details</td>\n            </tr>\n            \n            <tr>\n                <td><b>Name : </b>{{bank.account_name}}</td>\n                <td><b>Branch : </b>Ganapathypalayam</td>\n            </tr>\n            <tr>\n                <td><b>A/C No : </b>{{bank.bank_account_no}}</td>\n                <td><b>IFSC : </b>{{bank.branch_code}}</td>\n            </tr>\n            <tr>\n                <td><b>Bank : </b>{{bank.bank}}</td>\n                <td></td>\n            </tr>\n        </table>\n    </div>\n</div>\n\n</div>",
