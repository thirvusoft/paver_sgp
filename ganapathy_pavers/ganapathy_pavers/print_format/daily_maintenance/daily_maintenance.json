 "css": ".main {\n    width: 100%;\n    padding-bottom: 10px !important;\n}\n.left {\n    width: 50%;\n    float: left;\n}\n.right {\n    width: 50%;\n    float: right;\n    padding-left: 10px;\n}\n\n.right table,\nth,\ntd {\n    border: 1px solid black;\n    width: 100%;\n    text-align: center;\n    font-size: 15px;\n    font-weight: 900;\n}\n\n\n.main4 {\n    width: 100%;\n    padding-bottom: 10px !important;\n}\n.machine {\n    width: 70%;\n    float: left;\n    padding-bottom: 10px !important;\n}\n.vehicle {\n    width: 30%;\n    float: right;\n    padding-left: 10px;\n}\n\n.silo-line1 {\n    width: 50%;\n    text-align: center;\n    color: #e45829;\n}\n.silo-line2 {\n    color: #1dcc06;\n}\n.silo-item {\n    color: blue;\n}\ntable,\nth,\ntd {\n    width: 100%;\n    border: 1px solid black;\n    text-align: center;\n    font-size: 15px;\n    font-weight: 900;\n}\n",
