 "html": "<style>\n        table, td {\n            border: 1px solid black;\n        }\n\n</style>\n\n<div>\n    <table style=\"width: 100%;\">\n        <tr>\n            <td colspan=6 style=\"text-align:center\"  class=\"bg\" ><b>\n                {% set a=frappe.get_doc(\"Project\",doc.name) %}\n                {{doc.project_name}} \n            </b></td>\n        </tr>\n        \n        <tr><td colspan=6></td></tr>\n        <tr>\n            <td class=\"bg\"><b>ITEM</b></td>\n            <td><b>RATE</b></td>\n            <td><b>PRODUCTION RATE</b></td>\n            <td><b>NOS</b></td>\n            <td><b>SUPPLY SQFT</b></td>\n            <td><b>MEASUREMENT SQFT</b></td>\n        </tr>\n        {% set b=item_list(doc.name)%}\n        {% for i in b['items']%}\n        <tr >\n            <td ><b>{{i}}</b></td>\n            <td class=\"left\"><nobr>{{'%.1f' % b['items'][i]['rate']}} + GST</nobr></td>\n            <td class=\"left\">{{ \"%.2f\" % (b['production_rate'][i] or 0) }}</td>\n            <td class=\"left\">{{'%.1f' % b['items'][i]['nos']}}</td>\n            <td class=\"left\">{{'%.1f' %b['items'][i]['sqf']}}</td>\n            <td></td>\n        </tr>\n        {%endfor%}\n        <tr class=\"bg\">\n            <td colspan=4 style=\"text-align:center\"><b>TOTAL </b></td>\n            <td class=\"left\"><b>{{'%.1f' % b['supply_sqf']}}</b></td>\n            <td class=\"left\"><b>{{'%.1f' % b['sqf']}}</b></td>\n        </tr>\n        <tr>\n            <td colspan=6></td>\n        </tr>\n        <!--</table>-->\n        <!--<table>-->\n        <tr>\n            <td colspan=2></td>\n            <td><b>NOS</b></td>\n            <td><b>AMOUNT</b></td>\n            <td><b>PER SQFT (Measurement)</b></td>\n            <td><b>PER SQFT (Supply)</b></td>\n        </tr>\n        <tr>\n            <td colspan=2><b>Transport</b></td>\n            <td class=\"left\"></td>\n            <td class=\"left\">{{'%.1f' %doc.transporting_cost}}</td>\n            {% set tc =b['transporting_cost'] %}\n            <td class=\"left\">{{'%.1f' %tc}}</td>\n            <td class=\"left\">{{'%.1f' %tc}}</td>\n        </tr>\n        <tr>\n            <td colspan=2><b>JobWorker</b></td>\n            <td class=\"left\"></td>\n            <td class=\"left\">{{'%.1f' %doc.total_job_worker_cost}}</td>\n            {% set jwc=b['total_job_worker_cost'] %}\n            <td class=\"left\">{{'%.1f' %jwc}}</td>\n            <td class=\"left\">{{'%.1f' %jwc}}</td>\n        </tr>\n        {% set total_sqft_amount=0%}\n        {% for j in b['expense'] %}\n            {% if j.amount !=0%}\n                <tr>\n                    <td  colspan=2><b>{{j.description}}</b></td>\n                    <td class=\"left\">{{'%.1f' % j.nos or \"\"}}</td>\n                    <td class=\"left\">{{'%.1f' % j.amount }}</td>\n                    <td class=\"left\">{{'%.1f' % j.sqft_amount}}</td>\n                    <td class=\"left\">{{'%.1f' % j.supply_sqft_amount}}</td>\n                </tr>\n            {%endif%}\n        {%endfor%}\n        {% set total_sqft_amount=b['total'] %}\n        {% set total_expense = tc+jwc+total_sqft_amount %}\n        {% set supply_total_expense = tc+jwc+b['supply_total']%}\n        <tr class='bg'>\n            <td colspan=4 style=\"text-align:center\" ><b>TOTAL EXPENSES </b>\n            </td>\n            <td class=\"left\"><b>{{'%.2f' %total_expense}}</b></td>\n            <td class=\"left\"><b>{{'%.2f' %supply_total_expense}}</b></td>\n        </tr>\n    </table>\n</div>",
