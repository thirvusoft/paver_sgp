 "font": "Default",
 "html": "<style>\n        table, td {\n            border: 1px solid black;\n        }\n\n</style>\n\n<div>\n    <table >\n        <tr>\n            <td colspan=4 style=\"text-align:center\"  class=\"bg\" ><b>\n                {% set a=frappe.get_doc(\"Project\",doc.project_name) %}\n                {{doc.project_name}} \n            </b></td>\n        </tr>\n        \n        <tr><td colspan=4></td></tr>\n        <tr>\n            <td class=\"bg\"><b>PER SQ FT - 103+ GST</b></td>\n            <td><b>NOS</b></td>\n            <td><b>SUPPLY SQFT</b></td>\n            <td><b>MEASUREMENT SQFT</b></td>\n        </tr>\n        {% set b=item_list(doc.name)%}\n        {% for i in b['items']%}\n        <tr >\n            <td ><b>{{i}}</b></td>\n            <td class=\"left\">{{b['items'][i]['nos']}}</td>\n            <td class=\"left\">{{b['items'][i]['sqf']}}</td>\n            <td></td>\n        </tr>\n        {%endfor%}\n        <tr class=\"bg\">\n            <td colspan=2 style=\"text-align:center\"><b>TOTAL </b></td>\n            <td class=\"left\"><b>{{b['sqf']}}</b></td>\n            <td class=\"left\"><b>{{doc.measurement_sqft}}</b></td>\n        </tr>\n        <tr>\n            <td colspan=4></td>\n        </tr>\n        <tr>\n            <td></td>\n            <td><b>NOS</b></td>\n            <td><b>AMOUNT</b></td>\n            <td><b>PER SQFT</b></td>\n        </tr>\n        <tr>\n            <td><b>Transport</b></td>\n            <td class=\"left\"></td>\n            <td class=\"left\">{{doc.transporting_cost}}</td>\n                {% set tc =doc.transporting_cost /b['sqf'] %}\n            <td class=\"left\">{{'%.3f' %tc}}</td>\n        </tr>\n        <tr>\n            <td><b>JobWorker</b></td>\n            <td class=\"left\"></td>\n            <td class=\"left\">{{doc.total_job_worker_cost}}</td>\n            {% set jwc=doc.total_job_worker_cost/b['sqf'] %}\n            <td class=\"left\">{{'%.3f' %jwc}}</td>\n        </tr>\n        {% set total_sqft_amount=0%}\n        {% for j in b['expense'] %}\n        {% if j.amount !=0%}\n        <tr>\n            <td><b>{{j.description}}</b></td>\n            <td class=\"left\">{{j.nos or \"\"}}</td>\n            <td class=\"left\">{{j.amount }}</td>\n            <td class=\"left\">{{'%.3f' % j.sqft_amount}}</td>\n        </tr>\n        {%endif%}\n        {%endfor%}\n        {% set total_sqft_amount=doc.total/b['sqf'] %}\n        {% set total_expense = tc+jwc+total_sqft_amount%}\n\n        <tr class='bg'>\n            <td colspan=3 style=\"text-align:center\" ><b>TOTAL EXPENSES </b>\n            </td>\n            <td class=\"left\"><b>{{'%.3f' %total_expense}}</b></td>\n        </tr>\n    </table>\n</div>",
