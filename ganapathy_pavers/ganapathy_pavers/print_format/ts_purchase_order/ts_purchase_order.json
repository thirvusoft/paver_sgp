 "creation": "2022-04-18 12:12:02.232652",
 "css": ".master{\n    font-family:arial;\n    font-size:13px;\n}\n\n.master td{\n    font-family:arial;\n    font-size:13px;\n    color: black;\n}\n\n.tax td{\n    border-right:1px solid grey;\n}",
 "custom_format": 1,
 "default_print_language": "en-US",
 "disabled": 0,
 "doc_type": "Purchase Order",
 "docstatus": 0,
 "doctype": "Print Format",
 "font": "Default",
 "html": "<div>\n{% if letter_head and not no_letterhead -%}\n{% if print_settings.repeat_header_footer %}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{%- endif %}\n{%- endif %}\n\n<hr style=\"width;100%; border-bottom: 1px solid grey;\">\n\n<div class=\"master\" style=\"border: 1px solid grey;\">\n    <div style=\"width;100%; border-bottom: 1px solid grey; background-color: orange; font-size:15px; height:50px; border-top:13px solid orange;\n    padding-left: 13px\"><b>PURCHASE ORDER</b></div>\n    <div width=\"100%\">\n        <div style=\"float:left;\" align=\"left\" width=\"60%\">\n        </div>\n        <div align=\"left\" width=\"30%\" style=\"padding-right:15px; float:right;\">\n        <table width=\"100%\">\n            <tr>\n                <td width=\"80%\" style=\"white-space:nowrap;\" align=\"right\"><b>PO NO.</b> </td>\n                <td>:</td>\n                <td width=\"20%\">{{ doc.name }} <br></td>\n            </tr>\n            <tr>\n                <td width=\"80%\" align=\"right\"><b>Date</b> </td>\n                <td>:</td>\n                <td width=\"20%\">{{doc.get_formatted(\"transaction_date\")}}<br></td>\n            </tr>\n        </table>\n        </div>\n    </div>\n    <div width=\"100%\">\n        <table width=\"100%\">\n            <tr>\n                <td width=\"10%\"><b>Supplier: </b></td>\n                <td><b>{{ doc.supplier }}</b></td>\n            </tr>\n            <tr>\n                {% set supplier = frappe.get_doc(\"Supplier\", doc.supplier)%}\n                <td></td>\n                <td>{{ doc.address_display }}</td>\n            </tr>\n        </table>\n    </div>\n\n        <div style=\"padding:5px;\">\n        {% set serial_no=[] %}\n        <table style=\"border-top:1px solid grey;\">\n            {% set tablelength=10 %}\n            <tr style=\"border-bottom:1px solid grey; border-left:1px solid grey; border-right:1px solid grey\">\n                <td style=\"background-color:orange !important;\" align=\"center\">S.NO.</td>\n                <td style=\"border-left:1px solid grey;background-color:orange !important;\" align=\"center\">ITEM</td>\n                <td style=\"border-left:1px solid grey;background-color:orange !important;\" align=\"center\">QTY</td>\n               <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\">UOM</td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\">RATE</td>\n                <td style=\"border-left:1px solid grey;background-color:orange  !important;\" align=\"center\">AMOUNT</td>\n            </tr>\n\n            \n            {% for item in doc.items %}\n                {% set _=serial_no.append(1) %}\n            <tr style=\"border-bottom:1px solid grey; border-left:1px solid grey; border-right:1px solid grey\">\n                <td align=\"left\" width=\"6%\">{{ serial_no|length }}</td>\n                <td align=\"left\" style=\"border-left:1px solid grey;\" width=\"36%\">{{ item.item_name }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"10%\">{{ item.qty}}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"6%\">{{ item.uom }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\">{{ \"%.2f\"|format(item.rate) }}</td>\n                <td align=\"right\" style=\"border-left:1px solid grey;\" width=\"15%\">{{ \"%.2f\"|format(item.amount) }}</td>\n            </tr>\n            {% endfor %}\n\n                <tr style=\"border-bottom:0px\">\n                    <td colspan=\"2\" align=\"left\"><b>Total Qty : </b>{{ doc.total_qty }}</td>\n                    <td colspan=\"3\" align=\"right\"><b>Total Amount : </b></td>\n                    <td>{{ \"%.2f\"|format(doc.total) }}</td>\n                </tr>\n            </table>\n            </div>\n            <!--{% set sgstt = [] %}-->\n            <!--{% set cgstt = [] %}-->\n            <!--{% for gst in doc.ts_tax_breakup_table %}-->\n            <!--{% set _ = sgstt.append(gst.ts_state_amount) %}-->\n            <!--{% set _ = cgstt.append(gst.ts_central_amount) %}-->\n            <!--{% endfor %}-->\n            <div class=\"tax\" width=\"100%\" style=\"padding-right:15px;\">\n                <table style=\"border:1px solid grey; float:right; width:50%;\">\n                    <tr style=\"border-bottom:1px solid grey; background-color:orange\">\n                        <td align=\"center\">SNO.</td>\n                        <td align=\"center\">GST%</td>\n                        <td align=\"center\">Rate</td>\n                        <td align=\"center\">Amount</td>\n                    </tr>\n                    {% if doc.taxes|length == 2 %}\n                    <tr style=\"border:1px solid grey;\">\n                        <td align=\"center\">1</td>\n                        <td>CGST</td>\n                        <td align=\"right\">{{doc.taxes[0].rate}}     </td>\n                        <td align=\"right\">{{doc.taxes[0].tax_amount}}</td>\n                    </tr>\n                    <tr style=\"border:1px solid grey;\">\n                        <td align=\"center\">2</td>\n                        <td>SGST</td>\n                        <td align=\"right\">{{doc.taxes[1].rate}}</td>\n                        <td align=\"right\">{{doc.taxes[1].tax_amount}}</td>\n                    </tr>\n                    {% elif doc.taxes|length == 1 %}\n                    <tr style=\"border:1px solid grey;\">\n                        <td><b>1</b></td>\n                        <td>IGST</td>\n                        <td>{{doc.taxes[0].rate}}     </td>\n                        <td>{{doc.taxes[0].tax_amount}}</td>\n                    </tr>\n                    {% else %}\n                    {% endif %}\n                </table>\n            </div>\n            <!--{{doc.other_charges_calculation}}-->\n    <div>\n        <table width=100%>\n            <tr>\n                <td width=\"80%\" align=\"right\"><b>Subtotal : </b></td>\n                <td width=\"20%\">\n                {% if doc.grand_total %}\n                {{ \"%.2f\"|format(doc.grand_total) }}\n                {%endif%}\n                \n                \n                </td>\n            </tr>\n            <tr>\n                <td width=\"80%\" align=\"right\"><b>Roundoff : </b></td>\n                <td width=\"20%\">\n                {% if doc.rounding_adjustment %}\n                {{ doc.rounding_adjustment }}\n                {%endif%}\n                </td>\n            </tr>\n            <tr>\n                <td width=\"80%\" align=\"right\"><b>Grand Total : </b></td>\n                <td width=\"20%\">\n                {% if doc.rounded_total %}\n                {{ \"%.2f\"|format(doc.rounded_total) }}\n                {%endif%}\n                \n                </td>\n            </tr>\n            </table>\n            <table>\n            <tr>\n                <td width=\"100%\"><b>Amount in Words : </b>{{ doc.in_words }}</td>\n                <td width=\"0%\"><b></b></td>\n            </tr>\n            <tr>\n               \n                <td width=\"100%\"><b>Terms : </b>{{ doc.terms or \" \" }}</td>\n                <td width=\"0%\"></td>\n            </tr>\n            </table>\n        </div>\n            <div width=\"100%\" style=\"padding: 15px\">\n            <table width=\"100%\" style=\"padding: 15px\">\n            <tr>\n                <td width=\"50%\"></td>\n                <td width=\"50%\" style=\"border:1px solid grey;\"><b>For SRI GANAPATHY FLY ASH BRICKS</b></td>\n            </tr>\n            <tr>\n                <td width=\"50%\"></td>\n                <td width=\"50%\" style=\"border:1px solid grey;\"><br><br><br></td>\n            </tr>\n            </table>\n            </div>\n    \n    </div>\n</div>",
